#include<iostream>
using namespace std;
int main()
{
	int m=7;
	int n=6;
	int i,j,k;
    int a[]={10,20,30,40,50,60,70};	
    int b[]={10,30,40,50,60,45};
   	for(k=0;k<m;k++)
   	{
	   
	for(i=k;i<m;i++)
   	{
   	   for(j=k;j<i+1;j++)
		  {
		    cout<<a[j]<<" ";	
	      }	
	      cout<<"\n";
	}
}
}
